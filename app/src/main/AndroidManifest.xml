        <activity
            android:name=".presentation.view.activity.docente.fonico.NewEjercicioFonicoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.fonico.NewEjercicioFonicoDocenteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.AsignarEstudianteDeberActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.AsignarDeberDocenteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.view.activity.docente.Home_ActivitiesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Home_AppActivity"
            android:screenOrientation="portrait"
