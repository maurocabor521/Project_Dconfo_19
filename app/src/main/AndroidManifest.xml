<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.dconfo_app">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" /> <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/home"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".presentation.view.activity.docente.notas.PorcentNotasActivity"></activity>
        <activity
            android:name=".presentation.view.activity.docente.notas.NotasActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.ejercicios.MisEjerciciosActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.grupos.GrupoNuevoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.grupos.GrupoDocenteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.actividades.ActividadDocenteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.view.activity.docente.silabico.AsignarEjercicioSilabicoActivity"
